# Holus .env.example
# Copy to .env and fill in your values
# Used by scripts/setup.py and main.py if present

# LLM
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=qwen2.5:7b
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Notifications
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=
TWILIO_TO_NUMBER=

# Agents
JOB_HUNTER_ENABLED=true
JOB_HUNTER_SCHEDULE=every 6 hours
JOB_HUNTER_SALARY_MIN=150000
JOB_HUNTER_SALARY_MAX=180000
JOB_HUNTER_RESUME_PATH=~/Documents/resume.pdf
TRADING_MONITOR_EXCHANGE_BINANCE_API_KEY=
TRADING_MONITOR_EXCHANGE_BINANCE_API_SECRET=
SOCIAL_MEDIA_TWITTER_API_KEY=
SOCIAL_MEDIA_TWITTER_API_SECRET=
SOCIAL_MEDIA_TWITTER_ACCESS_TOKEN=
SOCIAL_MEDIA_TWITTER_ACCESS_TOKEN_SECRET=
INBOX_MANAGER_GMAIL_CREDENTIALS_PATH=~/.holus/gmail_credentials.json

# Memory
CHROMADB_PERSIST_DIR=~/.holus/memory

# Dashboard
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PORT=8080

# Logging
LOG_LEVEL=INFO
LOG_FILE=~/.holus/logs/holus.log
